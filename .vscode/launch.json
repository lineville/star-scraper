  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node run",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/index.js",
      "args": ["--org", "github", "--repo", "gh-valet", "--limit", "100000"]
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'star-scraper'",
      "cargo": {
        "args": [
          "build",
          "--bin=star-scraper",
          "--package=star-scraper"
        ],
        "filter": {
          "name": "star-scraper",
          "kind": "bin"
        }
      },
      "args": [
        "--org",
        "github",
        "--repo",
        "gh-valet",
        "--limit",
        "100"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "GITHUB_TOKEN": "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"
      }
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'star-scraper'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=star-scraper",
          "--package=star-scraper"
        ],
        "filter": {
          "name": "star-scraper",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
